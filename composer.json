{
    "name": "hyperf/database-pgsql-incubator",
    "type": "library",
    "description": "A pgsql handler for hyperf/database.",
    "license": "MIT",
    "keywords": [
        "php",
        "swoole",
        "hyperf",
        "database",
        "pgsql"
    ],
    "homepage": "https://hyperf.io",
    "support": {
        "docs": "https://hyperf.wiki",
        "issues": "https://github.com/hyperf/hyperf/issues",
        "pull-request": "https://github.com/hyperf/hyperf/pulls",
        "source": "https://github.com/hyperf/hyperf"
    },
    "authors": [
        {
            "name": "庄远辉",
            "email": "13580179149@163.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "hyperf/database": "^2.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "hyperf/di": "^2.2",
        "hyperf/framework": "^2.2",
        "hyperf/testing": "^2.2",
        "hyperf/utils": "^2.2",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": ">=7.0",
        "swoole/ide-helper": "dev-master"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Hyperf\\Database\\PgSQL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\Database\\PgSQL\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.2-dev",
        },
        "hyperf": {
            "config": "Hyperf\\Database\\PgSQL\\ConfigProvider"
        }
    }
}
