{
    "name": "zhuangyuanhui/database-pgsql-incubator",
    "type": "library",
    "require": {
        "php": ">=7.3",
        "hyperf/database": "^2.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "hyperf/di": "^2.1",
        "hyperf/testing": "^2.1",
        "hyperf/utils": "^2.1",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": ">=7.0",
        "swoole/ide-helper": "dev-master"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Hyperf\\Database\\PgSQL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\Database\\PgSQL\\": "tests"
        }
    },
    "authors": [
        {
            "name": "庄远辉",
            "email": "zhuangyuanhui@kkguan.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "hyperf": {
            "config": "Hyperf\\Database\\PgSQL\\ConfigProvider"
        }
    }
}
